{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "package": {
	  "productName": "ChatDOC",
	  "version": "1.0.0"
	},
	"tauri": {
	  "windows": [
		{
		  "url": "https://chatdoc.com/chatdoc/",
		  "transparent": false,
		  "fullscreen": false,
		  "width": 1200,
		  "height": 780,
		  "resizable": true
		}
	  ],
	  "security": {
		"csp": null
	  },
	  "updater": {
		"active": false
	  },
    "bundle": {
      "identifier": "com.PaodingAI.chatdoc"
    }
  },
	"build": {
	  "devPath": "../dist",
	  "distDir": "../dist",
	  "beforeBuildCommand": "",
	  "beforeDevCommand": ""
	}
  }
